{
    "mcpServers": {
        "mcp-server-fetch": {
            "command": "uvx",
            "args": [
                "mcp-server-fetch"
            ],
            "env": {
                "PATH": "/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"
            },
            "disabled": false,
            "autoApprove": [
                "fetch"
            ],
            "description": "Web content fetching and conversion for efficient LLM usage"
        }
    }
} 